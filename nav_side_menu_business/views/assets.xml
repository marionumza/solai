<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <!--FrontEnd Assets-->
    <template id="nav_side_menu_business_assets" name="Navbar Side Menu Assets"
              inherit_id="website.assets_frontend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/scss"
                  href="/nav_side_menu_business/static/src/scss/jquery.mCustomScrollbar.scss"/>
            <link rel="stylesheet" type="text/scss"
                  href="/nav_side_menu_business/static/src/scss/mid_header_off.scss"/>
            <script type="text/javascript"
                    src="/nav_side_menu_business/static/src/js/jquery.mCustomScrollbar.js"/>
            <script type="text/javascript"
                    src="/nav_side_menu_business/static/src/js/theme_custom.js"/>
        </xpath>
    </template>


    <template id="on_off_side_menu" inherit_id="website.assets_frontend"
              active="True">
        <xpath expr="." position="inside">
            <link href="/nav_side_menu_business/static/src/scss/mid_header_off.scss"
                  rel="stylesheet" type="text/scss"/>
        </xpath>
    </template>

    <!--Side Navbar Options 1-->
    <template id="option_side_nav_1"
              inherit_id="website._assets_primary_variables" active="True">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss"
                  href="/nav_side_menu_business/static/src/scss/style1.scss"/>
        </xpath>
    </template>
    <template id="option_side_nav_js_1" inherit_id="website.assets_frontend"
              active="True">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript"
                    src="/nav_side_menu_business/static/src/js/style1.js"/>
        </xpath>
    </template>

