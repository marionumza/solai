<?xml version="1.0" encoding="UTF-8"?>

<odoo>
<template id="demo_frontend_style" name="Theme Shop style"
              inherit_id="website.assets_frontend" active="True">
        <xpath expr="." position="inside">
            <link href="/business_snippet_blocks_crm/static/src/scss/crm.scss"
                  type="text/scss" rel="stylesheet"/>
            <link href="/business_snippet_blocks_crm/static/src/scss/contact_us_3.scss"
                  type="text/scss" rel="stylesheet"/>
            <link href="/business_snippet_blocks_crm/static/src/scss/contact_us_4.scss"
                  type="text/scss" rel="stylesheet"/>
            <link href="/business_snippet_blocks_crm/static/src/scss/contact_us_14.scss"
                  type="text/scss" rel="stylesheet"/>
            <script type="text/javascript"
                    src="/business_snippet_blocks_crm/static/src/js/crm.js"/>
        </xpath>
</template>
    <template id="snippet_id_crm" inherit_id="business_snippet_blocks_core.theme_ultimate_snippet_blocks_list">
